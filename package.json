{
  "name": "pesantren-api",
  "version": "1.0.0",
  "description": "pesantren api",
  "main": "server.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/server.js",
    "start-with-migrate_old": "npm run migrate && npm start",
    "start-with-migrate": "npm start",
    "dev": "concurrently \"tsc --watch\" \"nodemon -q dist/server.js\"",
    "lint:check": "eslint .",
    "lint:fix": "eslint --fix .",
    "format:check": "prettier --check .",
    "format:write": "prettier --write .",
    "make:migration": "sequelize-cli migration:generate --name",
    "db:migrate": "node ./node_modules/sequelize-cli/lib/sequelize db:migrate --env production --config src/database/sequelize.js --migrations-path dist/database/migrations --seeders-path dist/database/seeders",
    "db:migrate-undo": "node ./node_modules/sequelize-cli/lib/sequelize db:migrate:undo:all --env production --config src/database/sequelize.js --migrations-path dist/database/migrations",
    "make:seed": "sequelize-cli seed:generate --name",
    "db:seed": "node ./node_modules/sequelize-cli/lib/sequelize db:seed:all --env production --config src/database/sequelize.js --seeders-path dist/database/seeders",
    "db:seed-undo": "node ./node_modules/sequelize-cli/lib/sequelize db:seed:undo:all --env production --config src/database/sequelize.js --seeders-path dist/database/seeders",
    "migrate": "npm run db:migrate"
  },
  "keywords": [],
  "author": "YH",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.7.9",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.3",
    "cors": "^2.8.5",
    "csv-parse": "^6.1.0",
    "dotenv": "^16.4.7",
    "exceljs": "^4.4.0",
    "express": "^4.21.2",
    "express-async-errors": "^3.1.1",
    "express-fileupload": "^1.5.1",
    "express-xss-sanitizer": "^2.0.0",
    "jsonwebtoken": "^9.0.2",
    "moment": "^2.30.1",
    "mysql2": "^3.12.0",
    "node-cron": "^3.0.3",
    "nodemailer": "^6.9.16",
    "pg": "^8.16.0",
    "pg-hstore": "^2.3.4",
    "puppeteer": "^24.8.2",
    "querystring": "^0.2.1",
    "sequelize": "^6.37.5",
    "sequelize-typescript": "^2.1.6",
    "sharp": "^0.33.5",
    "tele-sender": "^1.0.2",
    "ts-node": "^10.9.2",
    "sequelize-cli": "^6.6.2",
    "uuid": "^11.0.5",
    "uuidv4": "^6.2.13",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/body-parser": "^1.19.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/express-fileupload": "^1.5.1",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/node": "^22.10.5",
    "@types/node-cron": "^3.0.11",
    "@types/nodemailer": "^6.4.17",
    "@types/sequelize": "^4.28.20",
    "@types/uuid": "^10.0.0",
    "concurrently": "^9.1.2",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^9.1.0",
    "nodemon": "^3.1.9",
    "prettier": "^3.4.2",
    "typescript": "^5.7.3"
  }
}
